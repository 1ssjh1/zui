{
  "extends": "./tsconfig.json",
  "include": [
    "./lib/*/docs/",
    "./lib/*/src/",
    "./lib/*/dev.ts",
    "./exts/*/*/docs/",
    "./exts/*/*/src/",
    "./exts/*/*/dev.ts",
    "scripts",
    "src",
    "dev",
    "config",
    "vite.config.ts",
    "tailwind.config.cjs",
    "postcss.config.cjs",
    ".eslintrc.cjs"
  ],
  "exclude": [
    "./lib/*/public/",
    "./exts/*/*/public/",
  ]
}
